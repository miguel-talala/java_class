package loopFor;

import java.util.Scanner;

public class ForLoopex {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		System.out.println("");
		int x = sc.nextInt();
		for(int i = 0; i <=x ; i ++) {
			if( x % 2 != 0) {
				System.out.println(x);
			}
		}
		sc.close();
	}
}
